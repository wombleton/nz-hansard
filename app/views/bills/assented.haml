- @title = "Bills: Aotearoa New Zealand's Parliament "
#normalcontent
  %h1 Assented Bills
  .divider3
  .contentarea
    %p
      %span Bills that received Royal Assent during the 48th Parliament.
      %span.right= %Q[See also: #{ link_to ('Negatived Bills', negatived_url) } | #{ link_to ('Current Bills', bills_url) }]
    %p= render(:partial => 'bill_group_link', :collection => @letter_to_assented.keys.sort, :locals => {:prefix => ''})
    %table
      %tr
        %th Bill name
        %th Party of MP in charge
        %th Bill type
      = render(:partial => 'bill_group', :collection => @letter_to_assented.keys.sort, :locals => {:letter_to_bills => @letter_to_assented, :bills => @bills_assented, :prefix => '', :first => @letter_to_assented.keys.sort[0]})
    .divider3
    .divider3
    %p= %Q[See also: #{ link_to ('Negatived Bills', negatived_url) } | #{ link_to ('Current Bills', bills_url) }]
